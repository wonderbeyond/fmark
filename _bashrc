# Description: alias for fmark
alias fma="fmark add"
alias fmg="fmark get"
alias fmls="fmark ls"
alias fmll="fmark ls -l"
alias fmd="fmark del"
alias fme="fmark get -e"

fmcd(){
    local mark="$1";
    local dir="`fmark get $mark`";

    [ ! -d "$dir" ] && dir=`dirname "$dir"`

    if [ -d "$dir" ]; then
        cd "$dir";
    else
        echo "not a dir for $mark";
    fi
}
